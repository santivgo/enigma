<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.projetosant.enigmafx.ApplicationController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="800.0">
         <image>
            <Image url="@img/bg.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="112.0" fitWidth="472.0" layoutX="371.0" layoutY="255.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@img/text.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="63.0" fitWidth="152.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@img/enigma-logo.png" />
         </image>
      </ImageView>
      <Button fx:id="btn_login" layoutX="611.0" layoutY="15.0" mnemonicParsing="false" onAction="#onBtnLoginClicked" style="-fx-background-color: None;" text="login">
         <font>
            <Font name="Londona" size="19.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="679.0" layoutY="15.0" mnemonicParsing="false" style="-fx-background-color: None;" text="cadastro">
         <font>
            <Font name="Londona" size="19.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
